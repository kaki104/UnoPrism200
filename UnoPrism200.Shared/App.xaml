<prismIoc:PrismApplication
    x:Class="UnoPrism200.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:prismIoc="using:Prism.DryIoc"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:UnoPrism200">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/_Colors.xaml"/>
                <ResourceDictionary Source="/Styles/_FontSizes.xaml"/>
                <ResourceDictionary Source="/Styles/_Thickness.xaml"/>
                <ResourceDictionary Source="/Styles/_TextBlock.xaml"/>
                <ResourceDictionary Source="/Styles/_Page.xaml"/>
                <ResourceDictionary Source="/Styles/_Button.xaml"/>
                <ResourceDictionary Source="/Styles/_Converters.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</prismIoc:PrismApplication>
